{
  "name": "merge-code-to",
  "displayName": "merge-code-to",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "merge-code.helloWorld",
        "title": "Hello World"
      }
    ],
    "configuration": {
      "title": "Git Merge Branch To",
      "properties": {
        "gitMergeBranchTo.branches": {
          "type": "array",
          "default": [],
          "description": "可以合并到哪些分支内？",
          "items": {
            "type": "string"
          }
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.88.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  },
  "volta": {
    "node": "20.10.0",
    "npm": "9.5.1"
  }
}
